{
	"do end block with params": {
		"prefix": "do",
		"body": [
			"do |${1:params}|",
			"  $0",
			"end"
		],
		"description": "Block with do..end and parameters"
	},
	"single line curly block": {
		"prefix": "ib",
		"body": [
			"{ |${1:params}| $0 }"
		],
		"description": "Single line block with {}"
	},
	"map(&:foo)": {
		"prefix": [
			"ma",
			"map"
		],
		"body": [
			"map(&:$1)$0"
		],
		"description": "Map over a collection with a block"
	},
	"let (RSpec)": {
		"prefix": "le",
		"body": [
			"let(:${1:name}) { $0 }"
		],
		"description": "RSpec let block"
	},
	"let! (RSpec)": {
		"prefix": "le!",
		"body": [
			"let!(:${1:name}) { $0 }"
		],
		"description": "RSpec let! block"
	},
	"describe (RSpec)": {
		"prefix": "des",
		"body": [
			"describe ${1:class_or_string} do",
			"  $0",
			"end"
		],
		"description": "RSpec describe block"
	},
	"context (RSpec)": {
		"prefix": "con",
		"body": [
			"context '${1:description}' do",
			"  $0",
			"end"
		],
		"description": "RSpec context block"
	},
	"it (RSpec)": {
		"prefix": "it",
		"body": [
			"it '${1:description}' do",
			"  $0",
			"end"
		],
		"description": "RSpec it block"
	}
}
